{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["@typescript-eslint"],
  "import/no-extraneous-dependencies": [
    "error",
    {
      "devDependencies": false,
      "optionalDependencies": false,
      "peerDependencies": false,
      "packageDir": "./"
    }
  ],
  "rules": {}
}
